package usageModif;

public class Usage2Tapis {
public static void main(float [] args) {
	float a=args[0];
	float b=args[1];
	float c=args[2];
	Tapis t2=new Tapis(a,b,c);
	float surfaceTapis=t2.calculerSurfaceTapis();
	float prixt2=t2.calculerPrixTapis(surfaceTapis);
	System.out.println("prix de t2 :"+prixt2);
	
	
}
}
