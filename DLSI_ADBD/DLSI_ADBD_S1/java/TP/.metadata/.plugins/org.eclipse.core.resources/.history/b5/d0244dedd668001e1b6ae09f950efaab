package testjava;
import java.util.Scanner;
public class comparasent {
 public static void main (String args[])
 {
	 Scanner in=new Scanner(Scanner.in);
	 int a=in.nextInt();
	 int b=in.nextInt();
	 int c=in.nextInt();
	 if (a>=b && a>=c) {
		 System.out.println("max_nb est ="+a);
		 
	 }
	 else 
		 if ((b>=a && b>=c))
			 System.out.println("max_nb est ="+b);
		 else
			 if((c>=a && c>=b))
				 System.out.println("max_nb est ="+c);
	 
 }
}
