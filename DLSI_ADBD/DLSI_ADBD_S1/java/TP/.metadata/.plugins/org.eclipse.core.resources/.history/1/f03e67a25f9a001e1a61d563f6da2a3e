package TD5;
import java.util.ArrayList;
public class Séminaire {
 String intitulé ;
 String période ;
 String lieu ; 
 Participant ind[];
 Participant uni[];
 public void inscrireParticipant(Participant participant) {
	 ind.add(participant);
     }
	public String toString() {
		return "intitulé :"+intitulé+" période: "+période+" lieu :"+lieu;
	}
}
